{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "headerKey": "X-Forwarded-For",
  "allowedOrigins": "http://localhost,http://localhost:4000",
  "allowedMethods": "GET,POST",
  "firstIpOnly": true,
  "git-repo": "https://github.com/gnairooze/client-ip.git",
  "help": "for direct client ip set header-key to empty value. allowedOrigins: comma separted values and can equal *. allowedMethods: comma separted values."
}
